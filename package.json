{
	"name": "@dimava/html-to-epub",
	"version": "1.0.1",
	"description": "A library to generate EPUB from HTML. Inspired by epub-gen.",
	"license": "MIT",
	"repository": "lesjoursfr/html-to-epub",
	"homepage": "https://github.com/lesjoursfr/html-to-epub#readme",
	"bugs": {
		"url": "https://github.com/lesjoursfr/html-to-epub/issues"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"engines": {
		"node": ">=12"
	},
	"scripts": {
		"build": "tsc",
		"watch": "tsc --watch",
		"test": "node test/test.js"
	},
	"files": [
		"index.js",
		"scr/**/*",
		"templates/**/*"
	],
	"keywords": [
		"epub",
		"generator",
		"ebook",
		"html"
	],
	"dependencies": {
		"archiver": "^5.3.0",
		"cheerio": "^1.0.0-rc.10",
		"diacritics": "^1.3.0",
		"ejs": "^3.1.6",
		"entities": "^3.0.1",
		"fs-extra": "^10.0.0",
		"mime": "^2.5.2",
		"superagent": "^6.1.0",
		"uslug": "^1.0.4"
	},
	"devDependencies": {
		"@types/archiver": "^5.1.1",
		"@types/cheerio": "^0.22.30",
		"@types/diacritics": "^1.3.1",
		"@types/ejs": "^3.1.0",
		"@types/entities": "^2.0.0",
		"@types/eslint": "^7.28.0",
		"@types/fs-extra": "^9.0.12",
		"@types/mime": "^2.0.3",
		"@types/node": "^16.9.1",
		"@types/superagent": "^4.1.12",
		"@types/uslug": "^1.0.1",
		"eslint": "^7.32.0",
		"typescript": "^4.4.2"
	}
}
